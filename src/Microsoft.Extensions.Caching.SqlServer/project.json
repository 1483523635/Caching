{
  "version": "1.0.0-rc2-final",
  "dependencies": {
    "Microsoft.Extensions.Caching.Abstractions": "1.0.0-rc2-final",
    "Microsoft.Extensions.Options": "1.0.0-rc2-final"
  },
  "description": "Distributed cache implementation of Microsoft.Extensions.Caching.Distributed.IDistributedCache using Microsoft SQL Server.",
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "frameworks": {
    "netstandard1.1": {
      "dependencies": {
        "System.Data.SqlClient": "4.1.0-rc2-24027"
      }
    }
  },
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/aspnet/caching"
    },
    "tags": [
      "cache",
      "distributedcache",
      "sqlserver"
    ]
  }
}